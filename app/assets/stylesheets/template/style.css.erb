/*
Colors:
E26623 - Orange medium brown
598527 - Forest Green
764B07 - Dark Brown
644D28 - Slightly Darker Brown

================================================================================
|                                     TAGS                                     |
================================================================================
*/
body,form,img{
	padding:0;
	margin:0;
	border:0;
}
body{
	background:#000000;
	color:#644D28;
	font-family:Arial;
	font-size:14px;
}
a{
	text-decoration:none;
	color:#E26623;
}
a:hover{
	text-decoration:none;
}
.fL{
	float:left;
}
.fR{
	float:right;
}
.cL{
	clear:both;
}
.tR{
	text-align:right;
}
#container {
	width: 1000px;
	margin: 0px auto;
}
/*
================================================================================
|                                     HEADER                                   |
================================================================================
*/
#header{
	width:1000px;
	text-align:left;
	height:511px;
	background:url('template/bg_header.gif') no-repeat 0 0;
}
#head_01{
	width:1000px;
	height:75px;
}
.flash{
	width:1000px;
	height:511px;
}
.menu{
	padding:21px 0 0 0;
}
.menu_1{
	width:250px;
	float:left;
}
.menu_2{
	width:245px;
	float:left;
}
.menu a{
	color:#764B07;
	font-size:15px;
	text-decoration:none;
	display:block;
	text-align:center;
	width:104px;
	height:24px;
	padding:7px 0 0 0;
	overflow:auto;
}
a.selected {
	color:#FFFFFF;
}
.selected,.menu a:hover{
	background:url('bg_menu_01.gif') no-repeat 0 0;
	color:#FFFFFF;
}
.slogan{
	font-size:11px;
	color:#764B07;
	padding:0 0 0 48px;
}
.brand{
	font-family:Times New Roman;
	font-size:35px;
	color:#764B07;
	line-height:38px;
/*	padding:19px 0 0 48px;*/
}
.h_pad_01{
	padding:2px 0 0 78px;
}
.ml_1{
	width:104px;
	padding:0 23px 0 0;
	float:left;
	background:url('template/menu_line.gif') no-repeat right 10px;
}
.ml_2{
	width:104px;
	padding:0 0 0 13px;
	float:left;
}
.ml_3{
	width:104px;
	padding:0 12px 0 6px;
	float:left;
	background:url('template/menu_line.gif') no-repeat right 10px;
}
.ml_4{
	width:104px;
	padding:0 0 0 19px;
	float:left;
}
.slog_box{
	width:352px;
	height:73px;
	float:left;
	text-align: center;
/*	overflow:auto;*/
}
/*
================================================================================
|                                     CONTENT                                  |
================================================================================
*/
#content{
	width:1000px;
	text-align:left;
	background:url('template/bg_content.gif') repeat-y 0 0;
}
.table{
	line-height:15px;
}
.table a{
	text-decoration:none;
	color:#644D28;
}
.table a:hover{
	text-decoration:underline;
}
.tRow1{
	background:url('template/bg_table.gif') repeat-y right 0;
	padding:1px 6px 0 6px;
}
.tRow2{
	padding:1px 4px 0 5px;
	width:457px;
}
.tRow3{
	background:url('template/bg_table.gif') repeat-y 0 0;
	padding:1px 9px 0 8px;
}
.tRow4{
	background:url('template/bg_table.gif') repeat-x 0 0;
	height:2px;
}
.list_01{
	padding:0;
	margin:0;
	list-style-type:none;
	line-height:18px;
}
.list_01 li{
	background:url('template/line_01.gif') repeat-x 0 bottom;
	padding:0 0 0 0;
}
.list_01 span{
	display:block;
	padding:7px 0 6px 15px;
	line-height:18px;
	background:url('template/marker_01.gif') no-repeat 0 13px;
}
.list_01 a{
	color:#644D28;
	text-decoration:none;
}
.list_01 a:hover{
	text-decoration:underline;
}
.pict_box_01{
	background:url('template/bg_img_01.gif') no-repeat 0 3px;
	padding:0 0 0 71px;
	color:#764B07;
	font-size:25px;
	font-family:Times New Roman;
}
.pict_box_01 span{
	font-size:35px;
	line-height:29px;
}
.c_text_01{
	line-height:17px;
}
.c_text_02{
	font-family:Times New Roman;
	font-size:25px;
	color:#764B07;
}
.c_text_03{
	line-height:17px;
	color:#598527;
}
.box_top_01{
	background:url('template/box_top_01.gif') no-repeat 0 0;
	width:84px;
}
.box_01{
	background:url('template/box_01.gif') repeat-y 0 0;
	width:84px;
}
.box_bottom_01{
	background:url('template/box_bottom_01.gif') no-repeat 0 bottom;
	width:84px;
}
.rm1{
	color:#FFFFFF;
	text-decoration:none;
	display:block;
	padding:4px 0 4px 0;
	text-align:center;
}
.rm1:hover{
	text-decoration:underline;
}
#latest-projects img {
	-moz-box-shadow: 5px 5px 5px #764B07;
	-webkit-box-shadow: 5px 5px 5px #764B07;
	box-shadow: 5px 5px 5px #764B07;
/*	-moz-box-shadow: 0 0 5px 5px #888;
	-webkit-box-shadow: 0 0 5px 5px #888;
	box-shadow: 0 0 5px 5px #888;*/
}
/*
================================================================================
|                                     FOOTER                                   |
================================================================================
*/
#footer{
	width:1000px;
	text-align:left;
	font-size: 12px;
}
.f_pad{
	padding:28px 0 27px 0;
}
.f_pad_01{
	padding:0 0 0 90px;
}
.f_pad_02{
	padding:0 20px 0 9px;
}
.l_foot{
	float:left;
	width:655px;
}
.r_foot{
	float:left;
	width:345px;
}
.f_text,.f_text a{
	color:#FFFFFF;
	text-decoration:none
}
.f_text span{
	padding:0 12px 0 12px;
}
.f_text a:hover{
	text-decoration:underline;
}
/*
================================================================================
|                                     FORMS                                    |
================================================================================
*/
.txb{
	width:255px;
	height:14px;
	border-right-style:solid;
	border-right-width:1px;
	border-bottom-width:1px;
	border-bottom-color:#D4D0C8;
	border-bottom-style:solid;
	border-right-color:#D4D0C8;
	background-color:#FFFFFF;
	border-left-color:#404040;
	border-top-color:#404040;
	font-size:12px;
	font-family:Tahoma;
	margin:0 0 5px 0;
	color:#644D28;
}
.area{
	width:535px;
	height:227px;
	border-right-style:solid;
	border-right-width:1px;
	border-bottom-width:1px;
	border-bottom-color:#D4D0C8;
	border-bottom-style:solid;
	border-right-color:#D4D0C8;
	background-color:#FFFFFF;
	border-left-color:#404040;
	border-top-color:#404040;
	font-size:12px;
	font-family:Tahoma;
	margin:0 0 1px 0;
	color:#644D28;
}
#reset{
	background:url('template/button_01.gif') no-repeat 0 0;
	height:23px;
	width:69px;
	border:0;
	vertical-align:top;
	cursor: pointer;
	margin:22px 15px 0 0;}
#submit{
	background:url('template/button_02.gif') no-repeat 0 0;
	height:23px;
	width:69px;
	border:0;
	vertical-align:top;
	cursor: pointer;
	margin:22px 17px 0 0;
}
